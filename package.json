{
  "name": "@artemsemkin/wp-headers",
  "version": "1.2.6",
  "description": "Generate and patch WordPress file headers (style.css, plugin PHP, readme.txt, TGM)",
  "license": "MIT",
  "author": "Artem Semkin",
  "repository": {
    "type": "git",
    "url": "https://github.com/artkrsk/wp-headers.git"
  },
  "homepage": "https://github.com/artkrsk/wp-headers#readme",
  "bugs": {
    "url": "https://github.com/artkrsk/wp-headers/issues"
  },
  "keywords": [
    "wordpress",
    "headers",
    "wp-headers",
    "style-css",
    "readme-txt",
    "plugin-header"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "3.2.4",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "packageManager": "pnpm@10.30.0"
}
